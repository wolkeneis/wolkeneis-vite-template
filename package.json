{
  "name": "@wolkeneis/<<NAME>>",
  "version": "0.0.0",
  "private": true,
  "description": "<<NAME>>",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wolkeneis/<<NAME>>.git"
  },
  "author": "Frooastside <frooastside@gmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/wolkeneis/<<NAME>>/issues"
  },
  "homepage": "https://<<NAME>>.wolkeneis.dev/",
  "engines": {
    "node": "*",
    "vscode": "^1.22.0",
    "npm": "^8.x.x",
    "pnpm": "7.x.x"
  },
  "main": "dist_electron/electron.js",
  "bin": {},
  "build": {
    "asar": true,
    "asarUnpack": true,
    "appId": "dev.wolkeneis.<<NAME>>",
    "productName": "<<NAME>>",
    "copyright": "Copyright Â© 2022 Wolkeneis",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": true,
      "allowToChangeInstallationDirectory": false
    },
    "files": [
      "dist/**/*",
      "dist_electron/**/*"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "build"
    }
  },
  "scripts": {
    "dev": "concurrently -k \"cross-env BROWSER=none pnpm vite:dev\" \"pnpm electron:dev\"",
    "build": "cross-env ELECTRON=true pnpm vite:build && pnpm electron:build && pnpm electron:bundle",
    "vite:dev": "vite",
    "vite:build": "tsc -p . && vite build",
    "vite:preview": "vite preview",
    "electron:dev": "wait-on tcp:3000 && nodemon -e ts -L -w electron --exec \"pnpm electron:build && cross-env IS_DEV=true pnpm electron .\"",
    "electron:build": "tsc -p electron --incremental",
    "electron:bundle": "electron-builder",
    "lint": "eslint --ext js,ts,jsx,tsx .",
    "lint:fix": "pnpm eslint --fix --ext js,ts,jsx,tsx .",
    "pretty": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.36",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react": "^1.3.0",
    "concurrently": "^7.2.1",
    "cross-env": "^7.0.3",
    "electron": "^19.0.1",
    "electron-builder": "^23.0.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.2",
    "nodemon": "^2.0.16",
    "prettier": "2.6.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "wait-on": "^6.0.1"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --ext js,ts,jsx,tsx --cache --fix",
    "*.{js,ts,jsx,tsx,css,md,json}": "prettier --write"
  }
}
